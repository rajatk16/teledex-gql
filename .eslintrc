{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "indent": ["error", 2],
    "comma-dangle": "off",
    "no-invalid-this": "off",
    "space-before-function-paren": "off",
    "object-curly-spacing": "off",
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreStrings": true
      }
    ]
  }
}
